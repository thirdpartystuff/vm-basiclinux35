    keyword \\\n yellow/24
    keyword $(*) yellow/24
    keyword ${*} brightgreen/16
    keyword linestart \t lightgray/13 red
    keyword whole @+@ brightmagenta/23 black/0
    keyword @+@ brightmagenta/23 black/0

